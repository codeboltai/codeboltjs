{
  "name": "remoteserverprovideragent",
  "private": true,
  "version": "1.0.3",
  "description": "Docker Provider Agent that connects to Codebolt application",
  "main": "dist/index.js",
  "scripts": {
    "build": "node esbuild.config.js",
    "build:vite": "vite build",
    "build:standalone": "node esbuild.config.js && npm run package:standalone",
    "package:standalone": "mkdir -p dist/node_modules && cp -r ../../node_modules dist/",
    "build:tsc": "tsc",
    "start": "node dist/index.js",
    "dev": "node esbuild.config.js && node dist/index.js",
    "dev:local": "node esbuild.config.js && node dist/index.js",
    "lint:test": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "docker",
    "codebolt",
    "agent",
    "websocket"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@codebolt/types": "workspace:*",
    "ws": "^8.18.0",
    "uuid": "^10.0.0",
    "js-yaml": "^4.1.0",
    "dockerode": "^4.0.2"
  },
  "devDependencies": {
    "@types/ws": "^8.5.13",
    "@types/uuid": "^10.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/dockerode": "^3.3.31",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "esbuild": "^0.19.0",
    "eslint": "^9.32.0",
    "typescript": "^5.7.2",
    "vite": "^5.0.0"
  }
}
