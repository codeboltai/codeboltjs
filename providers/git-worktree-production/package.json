{
  "name": "@codebolt/git-worktree-production",
  "version": "1.0.6",
  "description": "Production-ready Git Worktree Provider for CodeBolt",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "start-provider.js"
  ],
  "scripts": {
