  "dependencies": {
    "@codebolt/codeboltjs": "workspace:*",
    "@codebolt/provider": "workspace:*",
    "@codebolt/types": "workspace:*",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/ws": "^8.5.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
