{
  "name": "@codebolt/agent-creator-agent",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:bundle": "node esbuild.config.js",
    "build:single": "node build-single-file.js",
    "build:standalone": "node create-standalone.js",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "start:bundle": "node dist/agent-bundle.js",
    "start:single": "node dist/agent-single-file.cjs",
    "start:standalone": "node dist/index.cjs",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@codebolt/agent": "workspace:*",
    "@codebolt/agent-shared-nodes": "workspace:*",
    "@codebolt/codeboltjs": "workspace:*",
    "@codebolt/litegraph": "workspace:*",
    "@codebolt/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.27.0",
    "typescript": "^5.2.2",
    "webpack": "^5.103.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "private": true
}