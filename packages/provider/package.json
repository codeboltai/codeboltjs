{
  "name": "@codebolt/provider",
  "version": "1.0.0",
  "description": "Base provider utilities for CodeBOLT environment providers",
  "keywords": [
    "codebolt",
    "provider",
    "agent",
    "websocket"
  ],
  "author": "Codebolt",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint:test": "eslint src/**/*.ts && tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeboltai/codeboltjs.git",
    "directory": "packages/provider"
  },
  "homepage": "https://codeboltai.github.io",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@codebolt/types": "workspace:*",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/ws": "^8.5.13",
    "eslint": "^9.9.0",
    "typescript": "^5.4.5",
    "jest": "^29.7.0",
    "@codebolt/codeboltjs": "workspace:*"
  },
  "peerDependencies": {
    "@codebolt/codeboltjs": "workspace:*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  }
}


