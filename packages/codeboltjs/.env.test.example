# CodeboltJS Test Environment Configuration
# Copy this file to .env.test and update with your values

# ============================================================================
# WebSocket Configuration
# ============================================================================

# WebSocket server port (default: 12345)
SOCKET_PORT=12345

# WebSocket server URL (default: localhost)
CODEBOLT_SERVER_URL=localhost

# ============================================================================
# Agent Configuration (Optional)
# ============================================================================

# Agent ID for testing
agentId=test-agent

# Parent agent ID
parentId=test-parent

# Parent agent instance ID
parentAgentInstanceId=test-parent-instance

# Agent task identifier
agentTask=test-task

# ============================================================================
# Thread Configuration (Optional)
# ============================================================================

# Thread token for threaded conversations
threadToken=test-thread-token

# ============================================================================
# Development Settings
# ============================================================================

# Enable development mode
Is_Dev=true

# Enable debug logging for tests
DEBUG_TESTS=false

# ============================================================================
# Custom Environment Variables
# ============================================================================

# Any custom environment variables will be automatically passed through
# as URL parameters to the WebSocket connection
# CUSTOM_VAR=value
