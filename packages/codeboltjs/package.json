{
  "name": "@codebolt/codeboltjs",
  "version": "2.2.1",
  "description": "",
  "keywords": [],
  "author": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && node script/copy-wasm.js",
    "build:webpack": "webpack",
    "build:all": "npm run build && npm run build:webpack",
    "build:docs": "typedoc --plugin typedoc-plugin-missing-exports",
    "build:jsondocs": "typedoc --plugin typedoc-plugin-missing-exports --json out.json --pretty",
    "test": "echo \"Integration tests require WebSocket server - skipping for release\" && exit 0",
    "clean": "rm -rf dist build",
    "lint": "eslint src/**/*.ts && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeboltai/codeboltjs.git",
    "directory": "packages/codeboltjs"
  },
  "license": "MIT",
  "homepage": "https://codeboltai.github.io",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@codebolt/codeparser": "workspace:*",
    "@codebolt/types": "workspace:*",
    "@types/uuid": "^10.0.0",
    "buffer": "^6.0.3",
    "execa": "^9.5.2",
    "js-yaml": "^4.1.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "timers": "^0.1.1",
    "util": "^0.12.5",
    "uuid": "^11.1.0",
    "ws": "^8.17.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@codebolt/eslint-config": "workspace:*",
    "@types/events": "^3.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.2",
    "@types/uri-templates": "^0.1.34",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "jest-serial-runner": "^1.2.1",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.28.10",
    "typedoc-plugin-markdown": "^3.15.0",
    "typedoc-plugin-missing-exports": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testTimeout": 50000
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    }
  }
