{
  "name": "@codebolt/codeparser",
  "version": "1.2.1",
  "description": "Code parsing utilities with tree-sitter support for multiple programming languages",
  "keywords": [
    "parser",
    "tree-sitter",
    "ast",
    "code-analysis",
    "typescript",
    "javascript",
    "python",
    "rust",
    "go",
    "java",
    "cpp",
    "c",
    "csharp",
    "ruby",
    "php",
    "swift"
  ],
  "author": "Codebolt",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && node scripts/copy-wasm.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest --passWithNoTests",
    "dev": "tsc --watch",
    "lint:test": "eslint src/**/*.ts && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeboltai/codeboltjs.git",
    "directory": "packages/codeparser"
  },
  "license": "MIT",
  "homepage": "https://codeboltai.github.io",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@codebolt/globby": "^1.0.1",
    "web-tree-sitter": "^0.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "jest": "^29.7.0",
    "typescript": "^5.4.5"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  }
}
